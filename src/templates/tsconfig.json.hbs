{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "ESNext",
    "noEmitOnError": true,
    "removeComments": false,
    "outDir": "js",
    "disableReferencedProjectLoad": false,
    "target": "ESNext",
    "moduleResolution": "node",
    "isolatedModules": false,
    "sourceMap": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "lib": [
      "ESNext",
      "ES2019",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "pretty": true,
    "noEmit": false,
    "newLine": "crlf",
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "node_modules",
    "./models/**/*.ts"
  ],
  "include": [
    "./ts/**/*.ts"
  ]
}